{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "FastDFS": "Debug"
    }
  },
  "FastDFS": {
    "Clusters": {
      "default": {
        "TrackerServers": [
          "192.168.1.100:22122",
          "192.168.1.101:22122"
        ],
        "ConnectionPool": {
          "MaxConnectionPerServer": 50,
          "MinConnectionPerServer": 5,
          "ConnectionIdleTimeout": 300,
          "ConnectionLifetime": 3600,
          "ConnectionTimeout": 30000,
          "SendTimeout": 30000,
          "ReceiveTimeout": 30000
        },
        "NetworkTimeout": 30,
        "Charset": "UTF-8",
        "DefaultGroupName": "group1",
        "StorageSelectionStrategy": "TrackerSelection",
        "HttpConfig": {
          "ServerUrls": {
            "group1": "http://img1.example.com",
            "group2": "http://img2.example.com"
          },
          "DefaultServerUrlTemplate": "http://{ip}",
          "AntiStealTokenEnabled": true,
          "SecretKey": "FastDFS1234567890",
          "DefaultTokenExpireSeconds": 3600
        }
      },
      "backup": {
        "TrackerServers": [
          "192.168.2.100:22122"
        ],
        "ConnectionPool": {
          "MaxConnectionPerServer": 30,
          "MinConnectionPerServer": 3,
          "ConnectionIdleTimeout": 300,
          "ConnectionLifetime": 3600
        },
        "NetworkTimeout": 30,
        "Charset": "UTF-8"
      },
      "cdn": {
        "TrackerServers": [
          "cdn.example.com:22122"
        ],
        "ConnectionPool": {
          "MaxConnectionPerServer": 100,
          "MinConnectionPerServer": 10,
          "ConnectionIdleTimeout": 600,
          "ConnectionLifetime": 7200
        },
        "NetworkTimeout": 60,
        "Charset": "UTF-8",
        "StorageSelectionStrategy": "RoundRobin"
      }
    }
  }
}
